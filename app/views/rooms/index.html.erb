<p style="color: green"><%= notice %></p>

<h1>Rooms</h1>

<div id="rooms">
  <% @rooms.each do |room| %>
    <%= render room %>
    <p>
      <%= link_to "Show this room", room %>
    </p>
  <% end %>
</div>

<%= link_to "New room", new_room_path %>


<div class="container">
   <h3><%= current_user.email%> </h3>
   <h4>Users </h4>

   <% render @users %>

    <h4>Rooms </h4>

    <% render @rooms %>

</div>